<div>
  <h2>Sign Up</h2>  
  <%= form_with model: @user, url: sign_up_path do |f| %>
    <div>
      <%= f.label :email %>
      <%= f.email_field :email %>
    </div>
    <div>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>
    <div>
      <%= f.submit %>
    </div>
  <% end %>
</div>